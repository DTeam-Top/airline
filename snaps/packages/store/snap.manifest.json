{
  "version": "0.1.0",
  "description": "Airline Snap",
  "proposedName": "Airline",
  "repository": {
    "type": "git",
    "url": "https://github.com/DTeam-Top/airline"
  },
  "source": {
    "shasum": "h4/Rk6d7CvGoJKgQFgblgYzIAxQnhh7c41jLG4M/qJo=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
