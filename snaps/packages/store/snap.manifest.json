        {
          "expression": "*/10 * * * * *",
          "request": {
            "method": "alert"
          }
        },
        {
          "expression": "*/10 * * * * *",
          "request": {
            "method": "clean"
          }
        },
        {
          "expression": "*/10 * * * * *",
          "request": {
            "method": "fetch"
          }
        }
      ]
    }
  },
  "manifestVersion": "0.1"
